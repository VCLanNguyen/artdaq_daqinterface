services: {
  scheduler: {
    errorOnFailureToPut: false
  }
  NetMonTransportServiceInterface: {
    service_provider: NetMonTransportService
    broadcast_sends: true
    nonblocking_sends: true
    destinations: { }
  }
}

daq: {
  aggregator: {
    expected_fragments_per_event: 1
    max_event_size_bytes: 16777216
    print_event_store_stats: true
    broadcast_buffer_count: 5
	buffer_count: 5
    event_queue_wait_time: 5
    onmon_event_prescale: 1
    is_data_logger: true
    expected_art_event_processing_time_us: 100000
    restart_crashed_art_processes: false

	routing_token_config: {
		use_routing_master: false
	}

	auto_suppression_enabled: false
	sources: { }
  }

  metrics: {
    aggFile: {
      metricPluginType: "file"
      level: 3
      fileName: "/tmp/aggregator/agg_%UID%_metrics.log"
      uniquify: true
    }
  }
}

source: {
  module_type: NetMonInput
}
outputs: {
  rootNetOutput: {
    module_type: RootNetOutput
    #SelectEvents: { SelectEvents: [ pmod2,pmod3 ] }
  }

  normalOutput: {
    module_type: RootOutput
    fileName: "/tmp/artdaqdemo_r%06r_sr%02s_%#.root"
    fileProperties: { maxSubRuns: 1 maxRuns: 1 maxSize: 8192000 maxEvents: 1000 }
    fastCloning: false
    compressionLevel: 0
  }

}
physics: {
  analyzers: {


   checkintegrity: {
     module_type: CheckIntegrity
     raw_data_label: daq
     frag_type: TOY1
   }

  }

  producers: {

     BuildInfo:
     {
       module_type: ArtdaqDemoBuildInfo
       instance_name: ArtdaqDemo
     }
  }

  p2: [ BuildInfo ]
  my_output_modules: [ normalOutput ]
  my_mpi_output_modules: [ rootNetOutput ]
}
process_name: DAQDL
